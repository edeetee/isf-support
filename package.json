{
  "name": "isf-support",
  "displayName": "ISF Support",
  "description": "Basic syntax support for ISF files",
  "version": "0.0.2",
  "publisher": "EdwardTaylor",
  "engines": {
    "vscode": "^1.71.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edeetee/isf-support"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "isf-meta.injection",
        "injectTo": ["source.glsl"],
        "embeddedLanguages": {
          "meta.embedded.inline.json": "json"
        }
      }
    ]
  }
}
